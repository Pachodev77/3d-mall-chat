{
  "version": 2,
  "name": "3d-mall-chat",
  "builds": [
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "assets/**",
      "use": "@vercel/static"
    },
    {
      "src": "public/firebase-config.js",
      "use": "@vercel/static"
    },
    {
      "src": "firebase-config.js",
      "use": "@vercel/static"
    },
    {
      "src": "public/style.css",
      "use": "@vercel/static"
    },
    {
      "src": "public/furniture-persistence.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/firebase-config.js",
      "dest": "/firebase-config.js"
    },
    {
      "src": "/public/style.css",
      "dest": "/public/style.css"
    },
    {
      "src": "/public/furniture-persistence.js",
      "dest": "/public/furniture-persistence.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 